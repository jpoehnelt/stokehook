{
  "name": "stokehook",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "rimraf build public && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "^1.0.0-next.62",
    "@sveltejs/kit": "^1.0.0-next.345",
    "@types/html-minifier-terser": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "typescript": "~4.7.4"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.27.2",
    "firebase": "^9.9.0",
    "firebase-admin": "^10.2.0",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-outclick": "^3.1.0",
    "winston": "^3.8.1"
  }
}
